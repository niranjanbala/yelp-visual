<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

	<servlet>
		<servlet-name>updateBusiness</servlet-name>
		<servlet-class>open.source.kaggle.yelp.UpdateBusinessServlet</servlet-class>
	</servlet>		
	<servlet-mapping>
		<servlet-name>updateBusiness</servlet-name>
		<url-pattern>/yelp/data/business/update</url-pattern>
	</servlet-mapping>  
	<servlet>
		<servlet-name>loadBusiness</servlet-name>
		<servlet-class>open.source.kaggle.yelp.PutBusinessDataInSessionServlet</servlet-class>
	</servlet>		
	<servlet-mapping>
		<servlet-name>loadBusiness</servlet-name>
		<url-pattern>/yelp/data/business/load</url-pattern>
	</servlet-mapping>  
			<servlet>
		<servlet-name>findBusiness</servlet-name>
		<servlet-class>open.source.kaggle.yelp.FindBusinessServlet</servlet-class>
	</servlet>		
	<servlet-mapping>
		<servlet-name>findBusiness</servlet-name>
		<url-pattern>/yelp/data/business/find</url-pattern>
	</servlet-mapping>  
</web-app>
